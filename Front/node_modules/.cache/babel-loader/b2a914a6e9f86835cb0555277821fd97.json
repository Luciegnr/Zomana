BAAAA,OAAO,CAACC,GAAR,CAAYsB,QAAZ;AARoC;AAAA;;AAAA;AAAA;AAAA;;AAUpC,gBAAA,MAAI,CAACC,QAAL,CAAcC,KAAd,CAAoB,8BAApB;;AACAzB,gBAAAA,OAAO,CAACC,GAAR;;AAXoC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAavC,KA3CM;AA4CP4B,IAAAA,MA5CO,yBA4CY;AAAA,UAAVV,MAAU,SAAVA,MAAU;AACjBA,MAAAA,MAAM,CAAC,QAAD,CAAN;AACD,KA9CM;AAgDDW,IAAAA,aAhDC,gCAgDyBpB,OAhDzB,EAgDkC;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAnBS,gBAAAA,MAAmB,SAAnBA,MAAmB;AAAA;AAAA;AAAA,uBAEd7B,KAAK,CACzByC,GADoB,WACbxC,GADa,6CACgCmB,OAAO,CAACiB,IADxC,EAEpBN,IAFoB,CAEf,UAACC,GAAD,EAAS;AACbH,kBAAAA,MAAM,CAACG,GAAG,CAACK,IAAL,CAAN;AACA3B,kBAAAA,OAAO,CAACC,GAAR,CAAYqB,GAAZ;AACD,iBALoB,CAFc;;AAAA;AAE/BC,gBAAAA,QAF+B;AAQrCvB,gBAAAA,OAAO,CAACC,GAAR,CAAYsB,QAAZ;AARqC;AAAA;;AAAA;AAAA;AAAA;;AAUrC,gBAAA,MAAI,CAACC,QAAL,CAAcC,KAAd,CAAoB,sBAApB;;AACAzB,gBAAAA,OAAO,CAACC,GAAR;;AAXqC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaxC;AA7DM;AA5BG,CAAd;AA4FA,eAAeC,KAAf","sourcesContent":["import axios from \"axios\";\nconst url = \"http://localhost:3000/api/\";\nconst getId = JSON.parse(window.localStorage.getItem(\"user\"));\n\nvar user_id;\n\nif (getId == null) {\n  user_id = \"undefined\";\n} else {\n  user_id = getId.id;\n}\nconsole.log(user_id);\n\nconst users = {\n  namespaced: true,\n  state: {\n    auth: false,\n    role: [\"\"],\n    username: \"\",\n    id: \"\",\n  },\n\n  mutations: {\n    SIGNIN(state, payload) {\n      const user = {\n        username: payload.username,\n        email: payload.email,\n        role: payload.role,\n        id: payload.id,\n      };\n      localStorage.setItem(\"user\", JSON.stringify(user));\n      state.role = payload.role;\n      state.username = payload.username;\n      state.id = payload.id;\n      state.auth = true;\n    },\n    LOGOUT(state) {\n      localStorage.clear();\n      state.auth = false;\n    },\n  },\n  actions: {\n    async signup({ commit }, payload) {\n      try {\n        const response = await axios\n          .post(url + \"signup\", payload)\n          .then((res) => {\n            commit(\"SIGNUP\", response);\n            console.log(res);\n          });\n        console.log(response);\n      } catch (err) {\n        this.$snotify.error(\"Invalid Username or Password\");\n        console.log(err);\n      }\n    },\n\n    async signin({ commit }, payload) {\n      try {\n        const response = await axios\n          .post(url + \"signin-seller\", payload)\n          .then((res) => {\n            commit(\"SIGNIN\", res.data);\n            console.log(res);\n          });\n        console.log(response);\n      } catch (err) {\n        this.$snotify.error(\"Invalid Username or Password\");\n        console.log(err);\n      }\n    },\n    async signinClient({ commit }, payload) {\n      try {\n        const response = await axios\n          .post(url + \"signin-client\", payload)\n          .then((res) => {\n            commit(\"SIGNIN\", res.data);\n            console.log(res);\n          });\n        console.log(response);\n      } catch (err) {\n        this.$snotify.error(\"Invalid Username or Password\");\n        console.log(err);\n      }\n    },\n    logout({ commit }) {\n      commit(\"LOGOUT\");\n    },\n\n    async update_client({ commit }, payload) {\n      try {\n        const response = await axios\n          .put(`${url}modify-client/618259163378113e6475caaf`, payload.data)\n          .then((res) => {\n            commit(res.data);\n            console.log(res);\n          });\n        console.log(response);\n      } catch (err) {\n        this.$snotify.error(\"Can't modify product\");\n        console.log(err);\n      }\n    },\n  },\n};\nexport default users;\n"]}]}